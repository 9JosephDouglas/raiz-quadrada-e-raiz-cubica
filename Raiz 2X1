import math 
# Solicita ao usuário que digite os valores
num1 = float(input("Digite o primeiro valor: "))

# Calcula a soma dos dois valores
raiz_quadrada = num1**0.5  
raiz_cubica = math.pow(num1,1/3)

# Exibe o resultado da soma
print(f"A raiz quadrade de {num1} é {raiz_quadrada}") 
print(f"A raiz cubica de {num1} é {raiz_cubica}")
